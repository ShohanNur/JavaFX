<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hellofx.TextFieldController">
   <children>
      <Label fx:id="myLabel" layoutX="143.0" layoutY="40.0" text="Welcome">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label layoutX="188.0" layoutY="120.0" text="Enter Your Age ">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <TextField fx:id="myTextField" layoutX="159.0" layoutY="200.0" prefHeight="26.0" prefWidth="248.0" />
      <Button fx:id="myButton" layoutX="248.0" layoutY="280.0" mnemonicParsing="false" onAction="#submit" text="Submit " />
   </children>
</AnchorPane>
