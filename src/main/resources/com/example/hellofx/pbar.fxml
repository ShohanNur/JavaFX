   <children>
      <ProgressBar fx:id="myProgressBar" layoutX="185.0" layoutY="168.0" prefHeight="42.0" prefWidth="206.0" progress="0.0" />
      <Button fx:id="myButton" layoutX="262.0" layoutY="300.0" mnemonicParsing="false" onAction="#increaseProgress" text="Button" />
      <Label fx:id="myLabel" layoutX="262.0" layoutY="65.0" text="Label">
