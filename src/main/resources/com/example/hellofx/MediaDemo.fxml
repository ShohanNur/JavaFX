<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane prefHeight="615.0" prefWidth="825.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hellofx.mediaController">
   <children>
      <MediaView fx:id="mediaView" fitHeight="500.0" fitWidth="830.0" />
      <Button fx:id="playButton" layoutX="179.0" layoutY="555.0" mnemonicParsing="false" onAction="#playMedia" text="PLAY" />
      <Button fx:id="pausebutton" layoutX="361.0" layoutY="555.0" mnemonicParsing="false" onAction="#pauseMedia" text="PAUSE" />
      <Button fx:id="resetButton" layoutX="533.0" layoutY="555.0" mnemonicParsing="false" onAction="#resetMedia" text="RESET" />
   </children>
</AnchorPane>
